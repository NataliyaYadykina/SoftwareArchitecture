@model List<SPAWebApplication.Models.Employee>
<div>
    <h1>Home</h1>
    <button type="button" class="btn btn-primary">Add new</button>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Age</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var employee in Model)
            { 
            <tr>
                <th scope="row">@employee.Id</th>
                <td>@employee.Name</td>
                <td>@employee.Age</td>
            </tr>
            }
        </tbody>
    </table>
</div>